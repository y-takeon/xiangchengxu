<!--components/star/star.wxml-->
<view>
  <view class="star-wrapper">
    <view wx:for="{{result}}" wx:key="key" class="star-item">
      <image wx:if="{{item=='on'}}" src="/images/icon/star.png"></image>
      <image wx:else src="/images/icon/chat.png"></image>
    </view>
    <text>{{score}}</text>
  </view>
</view>
